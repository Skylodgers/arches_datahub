<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:prov="http://www.w3.org/ns/prov#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:dcat="http://www.w3.org/ns/dcat#">



    <rdfs:Class rdf:about="http://www.w3.org/ns/dcat#Resource">
        <rdfs:label>Resource</rdfs:label>
        <rdfs:comment>Representing any asset than can be included in the catalog</rdfs:comment>
    </rdfs:Class>


    <rdfs:Class rdf:about="http://www.w3.org/ns/dcat#Catalog">
        <rdfs:label>Catalog</rdfs:label>
        <rdfs:comment>A curated collection of metadata about datasets.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dcat#Resource"/>
    </rdfs:Class>

    <rdfs:Class rdf:about="http://www.w3.org/ns/dcat#Dataset">
        <rdfs:label>Dataset</rdfs:label>
        <rdfs:comment>A collection of data, published or curated by a single agent, and available for access or download in one or more formats.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dcat#Resource"/>
    </rdfs:Class>

    <rdfs:Class rdf:about="http://www.w3.org/ns/dcat#Distribution">
        <rdfs:label>Distribution</rdfs:label>
        <rdfs:comment>A specific representation of a dataset, such as a downloadable file.</rdfs:comment>
    </rdfs:Class>

    <rdfs:Class rdf:about="http://www.w3.org/ns/dcat#DataService">
        <rdfs:label>Data Service</rdfs:label>
        <rdfs:comment>A service that provides access to the data, such as an API or a SPARQL endpoint.</rdfs:comment>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dcat#Resource" />
    </rdfs:Class>

    <rdfs:Class rdf:about="http://www.w3.org/ns/dcat#CatalogRecord">
        <rdfs:label>Catalog Record</rdfs:label>
        <rdfs:comment>A record in a catalog, describing a dataset or a data service.</rdfs:comment>
    </rdfs:Class>


    <rdf:Property rdf:about="http://www.w3.org/ns/dcat#dataset">
        <rdfs:label>dataset</rdfs:label>
        <rdfs:comment>A dataset included in the catalog.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/dcat#Catalog"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
    </rdf:Property>

    <rdf:Property rdf:about="http://www.w3.org/ns/dcat#distribution">
        <rdfs:label>distribution</rdfs:label>
        <rdfs:comment>A distribution of the dataset.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/dcat#Distribution"/>
    </rdf:Property >

     <rdf:Property rdf:about="http://www.w3.org/ns/dcat#service">
        <rdfs:label>service</rdfs:label>
        <rdfs:comment>A service that provides access to the dataset.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/dcat#Catalog"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/dcat#DataService"/>
    </rdf:Property>

<!-- DCAT props-->

    <rdf:Property rdf:about="http://www.w3.org/ns/dcat#record">
        <rdfs:label>record</rdfs:label>
        <rdfs:comment>A record describing a dataset or service in the catalog.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/dcat#Catalog"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/dcat#CatalogRecord"/>
    </rdf:Property>

    <rdf:Property rdf:about="http://www.w3.org/ns/dcat#keyword">
        <rdfs:label>keyword</rdfs:label>
        <rdfs:comment>A keyword or tag describing the dataset.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </rdf:Property>

    <rdf:Property rdf:about="http://www.w3.org/ns/dcat#theme">
        <rdfs:label>theme</rdfs:label>
        <rdfs:comment>A category describing the dataset.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    </rdf:Property>

    <rdf:Property rdf:about="http://www.w3.org/ns/dcat#contactPoint">
        <rdfs:label>contact point</rdfs:label>
        <rdfs:comment>A contact point for the dataset.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    </rdf:Property>



    <!-- Properties -->
    <rdf:Property rdf:about="http://purl.org/dc/terms/title">
        <rdfs:label>title</rdfs:label>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/dcat#Resource"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal" />
    </rdf:Property>


    

</rdf:RDF>





